{
  "name": "empower-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host=0.0.0.0 --disable-host-check",
    "start": "node dist/server",
    "stat-dev": "PORT=8181 node dist/server",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "protractor e2e/protractor.conf.js",
    "webdriverupdate": "webdriver-manager update",
    "buildprod": "ng build --prod --source-map",
    "buildstaging": "ng build --configuration=staging",
    "build:client-and-server-bundles-prod": "ng build --prod && ng run empower-website:server",
    "build:client-and-server-bundles-staging": "ng build --configuration=staging && ng run empower-website:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "build:ssr-prod": "npm run build:client-and-server-bundles-prod && npm run compile:server",
    "build:ssr-staging": "npm run build:client-and-server-bundles-staging && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "serve:ssr": "node dist/server",
    "run:ssr": "npm run build:ssr && node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.2",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@types/googlemaps": "^3.30.12",
    "angular2-navigate-with-data": "^1.1.11",
    "bootstrap": "^4.2.1",
    "cookieconsent": "^3.0.6",
    "core-js": "^2.5.4",
    "domino": "^2.1.1",
    "express": "^4.16.4",
    "express-http-to-https": "^1.1.4",
    "font-awesome": "^4.7.0",
    "jwt-decode": "^2.2.0",
    "ngx-bootstrap": "^3.2.0",
    "ngx-cookie": "^4.0.2",
    "ngx-cookieconsent": "^2.0.0",
    "ngx-loading": "^1.0.14",
    "ngx-smart-modal": "^7.1.0",
    "raven-js": "^3.25.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^6.2.2",
    "spectre.css": "^0.5.1",
    "stellar-sdk": "^0.11.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.2.0",
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/rx": "^4.1.1",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.5.1",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mail-listener2": "^0.3.1",
    "pre-commit": "^1.2.2",
    "protractor": "^5.3.2",
    "rxjs-compat": "^6.1.0",
    "ts-loader": "^5.2.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.1.2"
  }
}
